{
	"name": "dofusdle",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"validate-data": "tsx scripts/validate-data.ts",
		"build": "npm run validate-data && tsc -b && vite build --mode production",
		"build:development": "npm run validate-data && tsc -b && vite build --mode development",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit && tsc -p tsconfig.test.json --noEmit",
		"preview": "vite preview",
		"dev:pages": "wrangler pages dev dist",
		"commitlint": "commitlint --edit",
		"prepare": "husky"
	},
	"dependencies": {
		"@sentry/react": "10.39.0",
		"canvas-confetti": "1.9.4",
		"fzf": "0.5.2",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-router": "7.13.1",
		"react-router-dom": "7.13.1",
		"zod": "3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@cloudflare/workers-types": "4.20260302.0",
		"@commitlint/cli": "20.4.1",
		"@commitlint/config-conventional": "20.4.1",
		"@sentry/vite-plugin": "4.9.1",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.2",
		"@testing-library/user-event": "14.6.1",
		"@types/canvas-confetti": "1.9.0",
		"@types/node": "24.10.12",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react": "5.1.3",
		"husky": "9.1.7",
		"jsdom": "28.0.0",
		"tsx": "4.19.4",
		"typescript": "5.9.3",
		"vite": "7.3.1",
		"vite-prerender-plugin": "0.5.12",
		"vitest": "4.0.18",
		"wrangler": "4.68.0"
	}
}
